<?xml version="1.0" encoding="UTF-8"?>
<Configuration status="warn" name="" packages="">
    <Appenders>
        <File name="MyFile" fileName="">
            <PatternLayout>
                <Pattern>%d{HH:mm:ss.SSS} [%t] %-5level %logger{36} - %msg%n</Pattern>
            </PatternLayout>
        </File>
    </Appenders>
    <Loggers>
        <Root level="trace">
            <AppenderRef ref="MyFile"/>
        </Root>
    </Loggers>
</Configuration>